{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": " Online Customer Locations in Bangalore",
    "projection": {"type": "equalEarth"},
    "layer": [
      {
        "data": {
          "url":"https://raw.githubusercontent.com/shyamsrini364/Vegalite/main/w8_q3_proportional_symbol_map-Bangalore/js/BBMP.geo%20(Topo).json",
          "format": {"type": "topojson", "feature": "BBMP.geo"}
        },
        "mark": {"type": "geoshape", "fill": "lightgreen", "stroke": "white"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/shyamsrini364/Vegalite/main/w8_q3_proportional_symbol_map-Bangalore/data/onlinedeliverydata.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "longitude", "type": "quantitative"},
          "latitude": {"field": "latitude", "type": "quantitative"},
          "size": {
            "field": "Family size",
            "type": "quantitative",
            "scale": {"domain": [1, 7]}

          },
          "color": {
            "field": "Occupation",
            "type": "nominal"
          }
        }
      }
    ]
  }

