{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 1000,
    "height": 1000,
    "title": " Online Customer Locations in Bangalore",
    "projection": {"type": "equalEarth"},
    "layer": [
      {
        "data": {
          "url":"https://raw.githubusercontent.com/shyamsrini364/Vegalite/main/w8_q3_proportional_symbol_map-Bangalore/js/BBMP.geo%20(Topo).json",
          "format": {"type": "topojson", "feature": "BBMP.geo"}
        },
        "mark": {"type": "geoshape", "fill": "lightgreen", "stroke": "grey"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/shyamsrini364/Vegalite/main/w8_q3_proportional_symbol_map-Bangalore/data/resturants.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "Longitude", "type": "quantitative"},
          "latitude": {"field": "Latitude", "type": "quantitative"},
          "color":{
            "field":"Dot",
            "type": "nominal"
          }

          }
        }
    ]
  }

